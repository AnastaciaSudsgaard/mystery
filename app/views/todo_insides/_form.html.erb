<%= form_with(model: [@todo_inside.todo_list, @todo_inside]) do |f| %>
  <% if @todo_inside.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@todo_inside.errors.count, "error") %> prohibited this todo_inside from being saved:</h2>

      <ul>
      <% @todo_inside.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </div>
  <div class="field">
    <%= f.label :completed %><br />
    <%= f.check_box :completed %>
  </div>
  <div class="field">
    <%= f.label :completed_at %><br />
    <%= f.datetime_select :completed_at %>
  </div>
  <div class="field">
    <%= f.label :todo_list %><br />
    <%= f.text_field :todo_list %>
  </div>
  <div class="field">
    <%= f.label :user %><br />
    <%= f.text_field :user %>
  </div>
  <%= f.hidden_field :book_id ,value: @todo_list.book_id %>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
